<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>

<VBox fx:controller="application.HisselerimController" xmlns:fx="http://javafx.com/fxml" spacing="10" alignment="CENTER" stylesheets="@styles.css" VBox.vgrow="ALWAYS">
    <children>
        <TableView fx:id="tableView" VBox.vgrow="ALWAYS">
            <columns>
                <TableColumn fx:id="nameColumn" text="Name"/>
                <TableColumn fx:id="miktarColumn" text="Miktar"/>
            </columns>
        </TableView>
    </children>
</VBox>