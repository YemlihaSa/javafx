<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" spacing="10" stylesheets="@styles.css" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.AnasayfaController">
    <children>
        <TableView fx:id="tableView" maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="400" prefWidth="700.0">
            <columns>
                <TableColumn fx:id="nameColumn" prefWidth="200" text="Name" />
                <TableColumn fx:id="alisColumn" prefWidth="200" text="Alis" />
                <TableColumn fx:id="satisColumn" prefWidth="200" text="Satis" />
            </columns>
         <VBox.margin>
            <Insets left="20.0" right="20.0" />
         </VBox.margin>
        </TableView>
    </children>
</VBox>
